#INCLUDE 'protheus.ch'
#INCLUDE 'parmtype.ch'

user function OPERADOR()

  local nNum1 := 10
  local nNum2 := 20

  /* Alert(nNum1 + nNum2)
  Alert(nNum2 - nNum1)
  Alert(nNum1 * nNum2)
  Alert(nNum2 / nNum1)
  Alert(nNum2 % nNum1) */

// OPERADORES RELACIONAIS
  Alert(nNum1 < nNum2)
  Alert(nNum1 > nNum2)
  Alert(nNum1 = nNum2)
  Alert(nNum1 == nNum2)
  Alert(nNum1 <= nNum2)
  Alert(nNum1 >= nNum2)
  Alert(nNum1 != nNum2)

return
